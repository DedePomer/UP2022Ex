<Window x:Class="UP2022Ex.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UP2022Ex"
        mc:Ignorable="d"
        Title="Приятный шелест" Height="450" Width="800" MinHeight="450" MinWidth="800" MaxHeight="450" MaxWidth="800" Icon="Resources\Приятный шелест.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="100"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal" Grid.Row="0" HorizontalAlignment="Center">
            <Image Source="Resources\Приятный шелест.png"/>
            <StackPanel VerticalAlignment="Center" Margin="10 0 0 0">
                <TextBlock FontSize="20">Приятный шелест</TextBlock>
            </StackPanel>
        </StackPanel>
        <Frame Grid.Row="1" Name="Fmain" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
